{
    "name": "delivery-app-back-end",
    "version": "0.1.0",
    "description": "Front-end do projeto App de Delivery",
    "engines": {
        "node": "16.14.2",
        "npm": "8.5.0"
    },
    "private": true,
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test -- --runInBand --detectOpenHandles --verbose",
        "test:coverage": "react-scripts test -- --coverage",
        "test:coverage:json": "react-scripts test -- --coverage --coverageReporters=\"json-summary\" --watchAll=false --testFailureExitCode=0 --detectOpenHandles --runInBand --forceExit",
        "eject": "react-scripts eject",
        "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json .",
        "lint:styles": "stylelint **/*.css --config .stylelintrc.json --ignore-disables --allow-empty-input"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.8.0",
        "@testing-library/jest-dom": "5.14.1",
        "@testing-library/react": "11.2.7",
        "@testing-library/user-event": "12.8.3",
        "axios": "^0.26.0",
        "joi": "^17.6.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-is": "^18.1.0",
        "react-redux": "^7.2.6",
        "react-router-dom": "^5.1.2",
        "react-scripts": "4.0.3",
        "redux-toolkit": "^1.1.2",
        "serve": "12.0.0",
        "socket.io-client": "^4.4.1",
        "styled-components": "^5.3.5",
        "web-vitals": "1.1.2"
    },
    "devDependencies": {
        "stylelint": "13.13.1",
        "stylelint-config-standard": "22.0.0",
        "stylelint-order": "4.1.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}"
        ],
        "coveragePathIgnorePatterns": [
            "index.js",
            "serviceWorker.js",
            "reportWebVitals.js",
            "./services/renderWithRouter",
            "./services/history"
        ]
    }
}
