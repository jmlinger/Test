{
  "version": 2,
  "builds": [
    {
      "src": "./src/api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "./src/api",
      "dest": "./src/api/server.js"
    },
    {
      "src": "./src/api",
      "methods": ["GET","POST"],
      "dest": "./src/api/server.js"
    }
  ]
}